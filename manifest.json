{
    "manifest_version": 3,
    "name": "Mobbin Image Enhancer",
    "version": "1.0",
    "description": "Am√©liore l'affichage des images sur mobbin.com",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.mobbin.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icons/froufrou_head.png",
      "48": "icons/froufrou_head.png",
      "128": "icons/froufrou_head.png"
    }
  }